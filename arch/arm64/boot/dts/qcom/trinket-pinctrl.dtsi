#include <dt-bindings/pinctrl/qcom,pmic-gpio.h>

&soc {
	tlmm: pinctrl@400000 {
		compatible = "qcom,trinket-pinctrl";
		reg = <0x400000 0xc00000>;
		interrupts = <0 227 0>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		};
};
